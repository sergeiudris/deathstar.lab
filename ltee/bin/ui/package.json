    "clean": "rm -rf target; rm -rf resources/public/js"
  },
  "dependencies": {
    "antd": "4.2.3",
    "create-react-class": "15.6.3",
    "less": "3.10.3",
    "less-plugin-clean-css": "1.5.1",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
